{
  "name": "toram-calculator",
  "type": "module",
  "scripts": {
    "backend:start": "docker compose --env-file .env -f ./backend/docker-compose.yaml up",
    "backend:stop": "docker compose --env-file .env -f ./backend/docker-compose.yaml stop",
    "backend:down": "docker compose --env-file .env -f ./backend/docker-compose.yaml down --volumes",
    "dev:db-ddl": "prisma migrate dev --name init --create-only --schema=db/clientDB/schema.prisma",
    "dev:db-type": "prisma generate --schema=db/clientDB/schema.prisma",
    "dev:db-push": "prisma db push --schema=db/clientDB/schema.prisma",
    "dev:db-studio": "prisma studio --schema=db/clientDB/schema.prisma",
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babylonjs/inspector": "^7.32.5",
    "@prisma/client": "^6.2.1",
    "@types/d3": "^7.4.3",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.9",
    "@types/pg": "^8.11.10",
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "pg": "latest",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.2.1",
    "prisma-kysely": "^1.8.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "zod-prisma-types": "latest"
  },
  "dependencies": {
    "@babylonjs/core": "^7.32.5",
    "@babylonjs/loaders": "^7.32.5",
    "@babylonjs/materials": "^7.32.5",
    "@editorjs/editorjs": "^2.30.4",
    "@electric-sql/client": "latest",
    "@electric-sql/next": "latest",
    "@electric-sql/pglite": "latest",
    "@electric-sql/pglite-sync": "latest",
    "@paralleldrive/cuid2": "^2.2.2",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.1",
    "@solidjs/start": "^1.0.2",
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/typography": "^0.5.14",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/solid-table": "^8.20.1",
    "@tanstack/solid-virtual": "^3.9.0",
    "blockly": "^11.2.1",
    "comlink": "^4.4.1",
    "d3": "^7.9.0",
    "drawflow": "^0.0.60",
    "fuse.js": "^7.0.0",
    "hotkeys-js": "^3.13.7",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "kysely": "^0.27.4",
    "lodash-es": "^4.17.21",
    "mathjs": "^13.0.3",
    "overlayscrollbars": "^2.10.0",
    "overlayscrollbars-solid": "^0.5.5",
    "sequential-workflow-designer": "^0.27.0",
    "solid-js": "^1.9.3",
    "solid-motionone": "^1.0.1",
    "solid-rete-plugin": "^0.1.18",
